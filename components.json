{
  "structuredData": {
    "@context": "https://schema.org",
    "@type": "WebSite",
    "name": "SecureDevOps",
    "url": "https://securedevops.site",
    "description": "Platformă educațională despre securitatea în DevOps.",
    "potentialAction": {
      "@type": "SearchAction",
      "target": "https://securedevops.site/search?q={search_term_string}",
      "query-input": "required name=search_term_string"
    }
  },
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "default",
  "rsc": true,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.ts",
    "css": "app/globals.css",
    "baseColor": "neutral",
    "cssVariables": true,
    "prefix": ""
  },
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils",
    "ui": "@/components/ui",
    "lib": "@/lib",
    "hooks": "@/hooks"
  },
  "iconLibrary": "lucide"
}